- title "Students"

%table
  %tr
    %th First Name
    %th Last Name
    %th Admin
    %th Editor
    %th State

  - for student in @students
    %tr
      %td= student.first_name
      %td= student.last_name
      %td= student.admin
      %td= student.editor
      %td= student.state
      %td= link_to "Edit", edit_student_path(student)

%p= link_to "New Student", new_student_path

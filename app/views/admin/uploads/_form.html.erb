<%= semantic_form_for [:admin, @upload], :html => {:multipart => true} do |f| %>
	<%= f.inputs do %>
		<%= f.input :label %>
		<%= f.input :file, :required => true, :as => :file %>
		<%= f.input :access, :label => 'Student Types', :as => :check_boxes, :collection => User.states %>
		<%= f.input :users, :label => 'Individual Students', :as => :check_boxes, :collection => User.all %>
	<% end %>
	
	<%= f.buttons do %>
		<%= f.commit_button 'Submit' %>
	<% end %>
<% end %>

<%= render 'navigation' %>